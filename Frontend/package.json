  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "test --coverage --watchAll=false",
    "collect-coverage": "cat coverage/lcov.info | coveralls",
    "lint:ts": "eslint --ext=ts,tsx src",
    "lint:css": "stylelint \"./src/**/*.{ts,tsx}\"",
    "compile": "tsc -p tsconfig.prod.json",
    "analyze": "source-map-explorer build/static/js/*.js",
    "prettier:check": "prettier --check src/**/*.{ts,tsx}",
    "checkAll": "yarn prettier:check & yarn lint:css & yarn lint:ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/**/*.{d.ts}",
      "!**/node_modules/**",
      "!src/__tests__/**",
      "!src/api/**",
      "!src/model/**",
      "!src/test/**",
      "!src/constants/**"
    ]
  }
