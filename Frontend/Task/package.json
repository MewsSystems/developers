{
  "name": "exchange-rate-client",
  "version": "1.0.0",
  "description": "Exchange Rate Client",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/MewsSystems/sample"
  },
  "scripts": {
    "start-server": "node server",
    "build-css": "lessc src/assets/main.less ./src/assets/main.css --js",
    "watch-css": "less-watch-compiler src/ ./src --enable-js --main-file ./assets/main.less",
    "start": "npm run watch-css & react-scripts start",
    "build": "rm -rf build && npm run build-css && react-scripts build"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-core": "6.13.2",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-stage-1": "6.13.0",
    "chance": "1.0.4",
    "express": "4.14.0",
    "json-loader": "0.5.4",
    "json-server": "0.8.17",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts": "^3.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "less": "^3.9.0",
    "less-watch-compiler": "^1.13.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
