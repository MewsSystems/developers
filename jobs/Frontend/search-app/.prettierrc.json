{
  "overrides": [
    {
      "files": "*",
      "options": {
        "arrowParens": "avoid",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "htmlWhitespaceSensitivity": "strict",
        "quoteProps": "consistent",
        "semi": true,
        "singleQuote": false,
        "trailingComma": "es5",
        "useTabs": false
      }
    }
  ]
}
