    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.1",
    "@tanstack/router-devtools": "^1.27.0",
    "@tanstack/router-vite-plugin": "^1.26.21",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
