  "paths": {
    "/cnbapi/exrates/daily": {
      "get": {
        "tags": [
          "/exrates"
        ],
        "summary": "Returns last valid data for selected day",
        "operationId": "dailyUsingGET_ExRatesDaily",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "date",
            "in": "query",
            "description": "Date in ISO format (yyyy-MM-dd); default value: NOW",
            "required": false,
            "type": "string",
            "format": "date",
            "x-example": "2019-05-17"
          },
          {
            "name": "lang",
            "in": "query",
            "description": "Language enumeration; default value: CZ",
            "required": false,
            "type": "string",
            "enum": [
              "CZ",
              "EN"
            ]
          }
        ],
        "responses": {
          "200": {
            "description": "Ok",
            "schema": {
              "$ref": "#/definitions/ExRateDailyResponse"
            }
          },
          "400": {
            "description": "Bad Request",
            "schema": {
              "$ref": "#/definitions/ErrorResponse"
            }
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/ErrorResponse"
            }
          }
        }
      }
    }
  },
