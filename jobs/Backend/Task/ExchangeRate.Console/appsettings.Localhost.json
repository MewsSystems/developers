{
  "ExchangeProviders": {
    "Cnb": {
      "Connection": {
        "Url": "https://www.cnb.cz/en/financial-markets/foreign-exchange-market/central-bank-exchange-rate-fixing/central-bank-exchange-rate-fixing/daily.txt"
      },
      "DataSourceFields": {
        "Amount": "Amount",
        "Currency": "Currency",
        "Rate": "Rate",
        "Country": "Country",
        "Code": "Code",
        "Separator": "|"
      },
      "Currencies": [
        "USD",
        "EUR",
        "CZK",
        "JPY",
        "KES",
        "RUB",
        "THB",
        "TRY",
        "XYZ"
      ],
      "Cache": {
        "IsEnabled": true,
        "TimeZoneId": "Central Europe Standard Time"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Warning",
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "%ASPNETCORE_SERILOG_FILE_PATH%",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ]
  }
}