{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "#{Serilog.MinimumLevel}#",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "restrictedToMinimumLevel": "#{Serilog.FileMinimumLevel}#",
          "path": "#{Serilog.FilePath}#",
          "rollingInterval": "#{Serilog.FileRollingInterval}#",
          "retainedFileCountLimit": "#{Serilog.RetainedFileCountLimit}#"
        }
      }
    ]
  },
  "AppConfiguration": {
    "AvailableCurrencies": "#{AvailableCurrencies}#"
  }
  "CzechNationalBankConfiguration": {
    "ApiBaseUrl": "#{CzechNationalBankConfiguration.ApiBaseUrl}#"
  }
}
