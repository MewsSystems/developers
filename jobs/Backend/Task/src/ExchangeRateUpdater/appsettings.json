{
    "ExchangeRateProviders" : {
        "CzechNationalBank": {
            "BaseUrl": "https://api.cnb.cz/cnbapi",
            "TimeoutSeconds": 30,
            "Cache": {
                "MonthlyRatesSlidingExpirationInMinutes": 1440,
                "MonthlyRatesAbsoluteExpirationInMinutes": 1440,
                "DailyRatesSlidingExpirationInMinutes": 1440,
                "DailyRatesAbsoluteExpirationInMinutes": 1440
            }
        }
    },
    "Caching": {
        "DailyRatesExpirationHours": 6,
        "MonthlyRatesExpirationDays": 1
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    }
}