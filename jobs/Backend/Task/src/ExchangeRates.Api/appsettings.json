{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Async", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "HttpClients": {
    "Cnb": {
      "BaseUrl": "https://api.cnb.cz",
      "DefaultTimeoutInMilliseconds": 1000
    }
  }
}
