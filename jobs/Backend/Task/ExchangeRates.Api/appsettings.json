{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Error",
      "System": "Error",
      "ExchangeRates": "Information"
    }
  },
  "AllowedHosts": "*",
  "CNBApi": {
    "HttpClient": {
      "BaseUrl": "https://api.cnb.cz/",
      "TimeoutSeconds": 10,
      "RetryCount": 2,
      "RetryBaseDelaySeconds": 2,
      "DailyRefreshTimeCZ": "14:30:00"
    }
  },
  "ExchangeRates": {
    "DefaultCurrencies": [ "USD", "EUR", "CZK", "JPY", "KES", "RUB", "THB", "TRY", "XYZ" ]
  }
}