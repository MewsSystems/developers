{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },

  "Database": {
    "UseInMemoryDatabase": false
  },

  "SystemConfiguration": {
    "BackgroundJobs": {
      "FetchHistoricalOnStartup": true,
      "HistoricalDataDays": 90
    }
  }
}
