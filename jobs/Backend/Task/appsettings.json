{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "ExchangeOptions": {
    "CnbRatesUrl": "https://api.cnb.cz/cnbapi/exrates/",
    "TargetCurrencyCode": "CZK",
    "RequiredCurrencyCodes": [
      "USD",
      "EUR",
      "CZK",
      "JPY",
      "KES",
      "RUB",
      "THB",
      "TRY",
      "XYZ"
    ]    
  }
}