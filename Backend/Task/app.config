<?xml version="1.0" encoding="utf-8"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1"/></startup>
  <appSettings>
    <add key="url" value="https://www.cnb.cz/cs/financni-trhy/devizovy-trh/kurzy-devizoveho-trhu/kurzy-devizoveho-trhu/denni_kurz.txt" />
    <add key="defaultCurrency" value="CZK" />
    <add key="decimalFormatProvider" value="cs-CZ" />
    <add key="lineSeparator" value="&#xA;" /> <!-- xml char for \n -->
    <add key="valueSeparator" value="|" />
    <add key="skippedRows" value="1" />
    <add key="rateColumnIndex" value="4" />
    <add key="quantityColumnIndex" value="2" />
    <add key="targetCurrencyColumnIndex" value="3" />
    <add key="expectedHeader" value="země|měna|množství|kód|kurz" />
  </appSettings>
</configuration>
